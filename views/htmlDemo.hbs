<div class="row">
  <div class="col-md-12">
    <br>
    <h2>Students</h2>
    <hr />
    <div class="table-responsive">
      <table class="table">
        <thead>
          <tr>
            <th>Student Num</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
          </tr>
        </thead>
        <tbody>
          {{#each students}}
          <tr>
            <td>{{this.studentNum}}</td>
            <td>{{this.firstName}}</td> 
            <td>{{this.lastName}}</td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
    {{#unless students}}
    <div class="col-md-12 text-center">
      <strong>{{message}}</strong>
    </div>
    {{/unless}}
  </div>
</div>